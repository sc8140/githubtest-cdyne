<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
    <Description>Default Proxy to create flows on an API</Description>
    <FaultRules/>
    <Flows/>
    <PostFlow name="PostFlow">
        <Request>
            <Step>
                <FaultRules/>
                <Name>XSLTransform-1</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>CompleteSoapRequest</Name>
            </Step>
        </Request>
        <Response/>
    </PostFlow>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <FaultRules/>
                <Name>GetZipCode</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>BuildXmlRequest</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>SaveOriginalVerb</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <HTTPProxyConnection>
        <BasePath>/forecast/v2/</BasePath>
        <Properties/>
        <VirtualHost>default</VirtualHost>
        <VirtualHost>secure</VirtualHost>
    </HTTPProxyConnection>
    <RouteRule name="default">
        <TargetEndpoint>default</TargetEndpoint>
    </RouteRule>
</ProxyEndpoint>
